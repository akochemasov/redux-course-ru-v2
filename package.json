      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "npx eslint --max-warnings=0",
      "prettier --write",
      "git add"
